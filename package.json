    "@metamask/onboarding": "1.0.1",
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "ress": "5.0.2",
    "web-vitals": "2.1.4",
    "web3": "1.7.4"
  },
