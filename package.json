    "test": "yarn jest",
    "lint": "yarn eslint src/ --ext ts,js,tsx,jsx,mjs,cjs --color",
    "format": "yarn prettier --write '**/*.{css,scss,json,md,html,yaml,yml,ts,js,tsx,jsx,cjs,mjs}'"
  },
